{
  "compilerOptions": {
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types"],
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"]
    }
  }
}
